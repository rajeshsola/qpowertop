mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: qpowertop.pro  /usr/lib/qt-3.3/mkspecs/default/qmake.conf /usr/lib/qt-3.3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile qpowertop.pro
qmake: 
	@$(QMAKE) -o Makefile qpowertop.pro

dist: 
	@mkdir -p .tmp/qpowertop && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/qpowertop/ && ( cd `dirname .tmp/qpowertop` && $(TAR) qpowertop.tar qpowertop && $(GZIP) qpowertop.tar ) && $(MOVE) `dirname .tmp/qpowertop`/qpowertop.tar.gz . && $(DEL_FILE) -r .tmp/qpowertop

